@model IEnumerable<PersonDataRandomizer.Models.ResultPerson>

@foreach (var person in Model)
{
    <div class="row" name="row">
        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">@person.FullName</div>
        <div class="col-lg-7 col-md-7 col-sm-7 col-xs-7">@person.Address</div>
        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">@person.PhoneNumber</div>
    </div>
}